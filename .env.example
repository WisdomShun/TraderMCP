# Interactive Brokers Gateway Configuration
IB_HOST=127.0.0.1
# 7497 for paper trading, 7496 for live trading
IB_PORT=7497
IB_CLIENT_ID=1
# Your IB account number
IB_ACCOUNT=All

# Database Configuration
CACHE_DB_PATH=./data/trading.db

# K-line data cache duration in days
CACHE_KLINE_DAYS=365

# Logging Configuration
LOG_LEVEL=INFO
LOG_PATH=./logs/
# 10MB
LOG_MAX_BYTES=10485760
LOG_BACKUP_COUNT=5

# Timezone
TIMEZONE=America/New_York
